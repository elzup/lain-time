{
	"name": "lain-time",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://lain.elzup.org",
	"scripts": {
		"offbuild": "node scripts/build.js",
		"test": "node scripts/test.js --env=jsdom",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"deploy": "up production",
		"deploy:url": "up url production",
		"coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"flow": "flow",
		"gen:env": "cp templates/temp.env .env.development.local && cp templates/temp.env .env.production.local",
		"precommit": "lint-staged",
		"storybook": "start-storybook -p 6006"
	},
	"dependencies": {
		"lodash": "^4.17.5",
		"moment": "^2.22.2",
		"query-string": "^6.1.0",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-image": "^1.3.1",
		"react-redux": "^5.0.6",
		"react-router-dom": "^4.2.2",
		"react-scripts": "^2.1.1",
		"redux": "^3.7.2",
		"redux-thunk": "^2.2.0",
		"styled-components": "^3.1.6"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0-beta.39",
		"@babel/preset-env": "^7.0.0-beta.39",
		"autoprefixer": "8.0.0",
		"babel-core": "6.26.0",
		"babel-engine-plugin": "^0.2.1",
		"babel-jest": "23.6.0",
		"babel-plugin-lodash": "^3.3.4",
		"babel-preset-react-app": "^3.1.1",
		"babel-runtime": "6.26.0",
		"brace-expansion-parser": "^1.0.0",
		"chalk": "2.3.1",
		"coveralls": "^3.0.0",
		"dotenv": "5.0.0",
		"flow-bin": "^0.74.0",
		"fs-extra": "5.0.0",
		"gh-pages": "^1.1.0",
		"husky": "^0.14.3",
		"jest": "23.6.0",
		"lint-staged": "^7.2.0",
		"prettier": "^1.13.6",
		"promise": "8.0.1",
		"puppeteer": "^1.5.0",
		"raf": "3.4.0",
		"react-dev-utils": "^5.0.0",
		"s2s": "^0.18.3",
		"s2s-plugins-redux": "^0.4.7"
	},
	"lint-staged": {
		"src/**/*.js": [
			"prettier --config ./.prettierrc --write",
			"git add"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
